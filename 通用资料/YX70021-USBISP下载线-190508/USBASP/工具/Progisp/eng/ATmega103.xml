				<NAME>CKSEL1</NAME>
				<TEXT>Select Clock Source</TEXT>
				<DEFAULT>0</DEFAULT>
			</FUSE1>
			<FUSE0>
				<NAME>CKSEL0</NAME>
				<TEXT>Select Clock Source</TEXT>
