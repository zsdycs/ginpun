		</TEXT1>
		<TEXT2>
			<MASK>0x06</MASK>
			<VALUE>0x04</VALUE>
			<TEXT>Mode 2: Further programming disabled</TEXT>
		</TEXT2>
		<TEXT3>
			<MASK>0x06</MASK>
			<VALUE>0x00</VALUE>
			<TEXT>Mode 3: Further programming and verification disabled</TEXT>
		</TEXT3>
		<LOCKBIT0>
			<NAME>LB1</NAME>
			<TEXT>Lockbit</TEXT>
		</LOCKBIT0>
		<LOCKBIT1>
