			</TEXT1>
			<TEXT2>
				<MASK>0x08</MASK>
				<VALUE>0x00</VALUE>
				<TEXT>Preserve EEPROM memory through the Chip Erase cycle</TEXT>
			</TEXT2>
			<TEXT3>
				<MASK>0x03</MASK>
				<VALUE>0x00</VALUE>
				<TEXT>SUT=00 Start-up time=5 CPU cycles</TEXT>
			</TEXT3>
			<TEXT4>
				<MASK>0x03</MASK>
				<VALUE>0x01</VALUE>
				<TEXT>SUT=01 Start-up time=0.5 ms</TEXT>
			</TEXT4>
			<TEXT5>
				<MASK>0x03</MASK>
				<VALUE>0x02</VALUE>
				<TEXT>SUT=10 Start-up time=4.0 ms</TEXT>
			</TEXT5>
			<TEXT6>
				<MASK>0x03</MASK>
				<VALUE>0x03</VALUE>
