		</LOCKBIT2>
		<LOCKBIT3>
			<NAME>BLB02</NAME>
			<TEXT>Boot Lock bit</TEXT>
		</LOCKBIT3>
		<LOCKBIT4>
			<NAME>BLB11</NAME>
			<TEXT>Boot lock bit</TEXT>
		</LOCKBIT4>
		<LOCKBIT5>
			<NAME>BLB12</NAME>
			<TEXT>Boot lock bit</TEXT>
		</LOCKBIT5>
	</LOCKBIT>
