<AVRPART>	
  <ADMIN>
    <PART_NAME>ATmega324</PART_NAME>
    <SPEED>16MHZ</SPEED>
    <SIGNATURE>
      <ADDR000>0x1E</ADDR000>
      <ADDR001>0x95</ADDR001>
      <ADDR002>0x11</ADDR002>
    </SIGNATURE>   
  </ADMIN>
  <PROGRAMMING>
