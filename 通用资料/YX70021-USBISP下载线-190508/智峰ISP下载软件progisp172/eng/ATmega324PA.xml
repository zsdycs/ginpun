    <LockRPos>3</LockRPos>    
    <LockWCMD>0xace00000</LockWCMD>    
    <LockWPos>3</LockWPos>  
    <fusevalue>0xff9972</fusevalue>  
    <FuseMask>0xffdfff</FuseMask>        
